<h1>Users</h1>

<%= link_to "New User", new_user_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td>
          <%= link_to "Players", user_players_path(user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>